<script>
        document.addEventListener('click', function (event) {
    // Verificar si el clic provino de una tarjeta de cliente
    if (event.target.dataset.customerId) {
        const customerId = event.target.dataset.customerId;
        const customerName = event.target.querySelector('.text-dark').textContent;
        const customerAddress = event.target.querySelector('.mdi-map + span').textContent;
        const emailAddress = event.target.querySelector('.mdi-email + span').textContent;
        const phoneNumber = event.target.querySelector('.mdi-phone + span').textContent;

        // Llenar los elementos del nuevo modal "modal-contact-new" con los datos del cliente
        document.getElementById('customer-name-new').textContent = customerName;
        document.getElementById('customer-email-address-new').textContent = emailAddress;
        document.getElementById('customer-phone-number-new').textContent = phoneNumber;
        document.getElementById('customer-address-new').textContent = customerAddress;
        document.getElementById('customer-id-new').textContent = customerId;

        // Establecer el atributo data-customer-id en el botón Eliminar para que pueda accederse más adelante
        document.querySelector('.btn-delete-new').setAttribute('data-customer-id', customerId);

        // Mostrar el nuevo modal "modal-contact-new"
        $('#modal-contact-new').modal('show');
    }
});


        function deleteCustomer(customerId) {
    // Realiza una solicitud HTTP DELETE a la API para eliminar el cliente
    $.ajax({
        url: `https://localhost:7259/api/Customer/${customerId}`,
        type: 'DELETE',
        success: function (data) {
            // Cliente eliminado exitosamente, puedes realizar alguna acción adicional si es necesario
            console.log(`Cliente con ID ${customerId} eliminado.`);
            // Recarga la página para actualizar la lista de clientes (puedes hacerlo de manera más eficiente actualizando la vista sin recargar)
            location.reload();
        },
        error: function (error) {
            // Ocurrió un error al eliminar el cliente
            console.error(`Error al eliminar el cliente con ID ${customerId}: ${error.responseText}`);
        }
    });
}

$(document).ready(function () {
    $('#modal-contact-new .btn-delete-new').click(function () {
        var customerId = $('#modal-contact-new #customer-id-new').text();
        deleteCustomer(customerId);
    });
});
</script>

